{
    "name": "katoba86/yii2-configuration",
    "description": "Configuration kit for Yii applications",
    "keywords": ["yii2", "kit", "configuration", "project template"],
    "homepage": "https://2amigos.us/",
    "type": "extension",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/2amigos/yii2-config-kit/issues?state=open",
        "source": "https://github.com/2amigos/yii2-config-kit"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.1",
        "league/container": "^3.3",
        "vlucas/phpdotenv": "^4.0",
        "symfony/console": "^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "*"

    },

    "config": {
        "process-timeout": 1800
    },
    "autoload" : {
        "psr-4": {
            "SideKit\\Config\\": ["./src", "./tests/"]
        }
    }
}
