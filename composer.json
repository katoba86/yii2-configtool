{
    "name": "katoba86/yii2-configuration",
    "description": "Configuration kit for Yii applications",
    "keywords": ["yii2", "kit", "configuration", "project template","config","environment"],
    "homepage": "https://fahrplan-bus-bahn.de",
    "type": "extension",
     "authors": [
        {
            "name": "Kai Bartholome",
            "email": "kai.bartholome@outlook.com",
            "homepage": "https://fahrplan-bus-bahn.de",
            "role": "Developer"
        }
    ],
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/katoba86/yii2-configuration/issues?state=open",
        "source": "https://github.com/katoba86/yii2-configuration"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.1",
        "league/container": "^3.3",
        "vlucas/phpdotenv": "^4.0",
        "symfony/console": "^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "*"

    },

    "config": {
        "process-timeout": 1800
    },
    "autoload" : {
        "psr-4": {
            "SideKit\\Config\\": ["./src", "./tests/"]
        }
    }
}
